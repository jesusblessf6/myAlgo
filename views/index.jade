extends layout

block content
	link(rel='stylesheet', href='/stylesheets/mystyle.css')
	//- h1= title
	//- p Welcome to #{title}
	body
		.site-wrapper
			.site-wrapper-inner
				.cover-container
					.masthead.clearfix
						.inner
							h3.masthead-brand Cover
							ul.nav.masthead-nav
								li.active: a(href='#') Home
								li: a(href='#') Feature
								li: a(href='#') Contact

					.inner.cover
						h1.cover-heading Conver your page.
						p.lead Cover is a one-page template for building simple and beautiful home pages. Download, edit the text, and add your own fullscreen background photo to make it your own.
						p.lead: a.btn.btn-lg.btn-default(href='#') Learn more